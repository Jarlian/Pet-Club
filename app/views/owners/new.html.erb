<!-- TODO: Mostrar los errores del formulario cuando hayan errores -->
<h2>Inscribir nueva mascota.</h2> <!-- TODO: Correjir titulo -->
<%= form_with model: @owner do |form| %>
  <div>
    <%= form.label :nombre %>
    <%= form.text_field :nombre %>
  </div>

  <div>
    <%= form.label :cedula %>
    <%= form.number_field :cedula %>
  </div>

  <div>
    <%= form.label :telefono %>
    <%= form.telephone_field :telefono %>
  </div>

  <div>
    <%= form.label :correo %>
    <%= form.email_field :correo %>
  </div>
  <%= form.submit "Inscribir persona" %>
<% end %>
